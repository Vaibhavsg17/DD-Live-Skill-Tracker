{
    "manifest_version": 3,
    "name": "User Activity Tracker",
    "version": "1.0",
    "permissions": [
        "tabs",
        "storage",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {}
}